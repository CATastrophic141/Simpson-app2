            <Font name="System Italic" size="10.0" />
         </font>
      </TextField>
      <TextField fx:id="itemValueField" layoutX="332.0" layoutY="525.0" promptText="Value (USD)" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="330.0">
