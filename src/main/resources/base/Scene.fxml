      </ImageView>
      <Button fx:id="selectButton" layoutX="16.0" layoutY="486.0" mnemonicParsing="false" onAction="#selectItemFromList" text="Select" AnchorPane.bottomAnchor="84.0" AnchorPane.leftAnchor="14.0" />
